<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.eclipse.sirius.componentization">
    <component class="at.tb_gruber.designer.design.Activator"
               id="at.tb_gruber.designer.design"
	       name="designer">
    </component>
  </extension>
  <extension
        point="org.eclipse.ui.newWizards">
     <category
           id="at.tb_gruber.designer.ide.category1"
           name="TB Gruber">
     </category>
     <wizard
           category="at.tb_gruber.designer.ide.category1"
           class="at.tb_gruber.designer.ide.NewWizard"
           id="at.tb_gruber.designer.ide.wizard1"
           name="TBG Projekt"
           project="true">
     </wizard>
  </extension>
  <extension
        id="tbgnature"
        name="TBG Nature"
        point="org.eclipse.core.resources.natures">
     <runtime>
        <run
              class="at.tb_gruber.designer.ide.ProjectNature">
        </run>
     </runtime>
  </extension>
  <extension point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
 	<editpartProvider class="at.tb_gruber.designer.design.style.StyleEditPartProvider">
 	  <Priority name="High"/> 
 	</editpartProvider> 
  </extension>
  <extension
        point="org.eclipse.gmf.runtime.common.ui.services.action.globalActionHandlerProviders">
     <GlobalActionHandlerProvider
           class="at.tb_gruber.designer.ide.print.TBGDiagramWithPrintGlobalActionHandlerProvider"
           id="at.tb_gruber.designer.design.GlobalActionHandlerProvider">
        <Priority
              name="Lowest">
        </Priority>
        <ViewId
              id="org.eclipse.sirius.diagram.ui.part.SiriusDiagramEditorID">
           <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="print"/>
            </ElementType>
        </ViewId>
     </GlobalActionHandlerProvider>
  </extension>
</plugin>
